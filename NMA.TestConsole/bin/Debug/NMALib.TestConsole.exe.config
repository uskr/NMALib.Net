<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <section name="nmaClient"
             type="NMALib.NMAClientConfiguration, NMALib" />
  </configSections>

  <!--
  Configure the NMA client below.
  You can retrieve your NMA API key at http://nma.usk.bz.
